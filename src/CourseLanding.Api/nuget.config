<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <!-- Clear fallback folders so Linux/CI builds don't fail looking for Windows paths.
       Workaround for: https://github.com/dotnet/sdk/issues/42421 -->
  <fallbackPackageFolders>
    <clear />
  </fallbackPackageFolders>
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
  </packageSources>
</configuration>
